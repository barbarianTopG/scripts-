local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")
local Workspace = game:GetService("Workspace")
local uiLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/laderite/bleklib/main/library.lua"))()
local function getGameName()
	local universeId = game.GameId;
	local success, result = pcall(function()
		return HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games?universeIds=" .. universeId))
	end)
	if success and result and result.data and #result.data > 0 then
		return result.data[1].name
	else
		return "Unknown Game"
	end
end;
local function createEspLabel(target, text)
	if not target or target:FindFirstChild("ESP_Label") then
		return
	end;
	local billboard = Instance.new("BillboardGui")
	billboard.Name = "ESP_Label"
	billboard.Adornee = target;
	billboard.Size = UDim2.new(4, 0, 1, 0)
	billboard.StudsOffset = Vector3.new(0, 3, 0)
	billboard.AlwaysOnTop = true;
	local textLabel = Instance.new("TextLabel")
	textLabel.Parent = billboard;
	textLabel.Size = UDim2.new(0, 200, 0, 25)
	textLabel.Position = UDim2.new(0.5, -100, 0, -12)
	textLabel.BackgroundTransparency = 1;
	textLabel.Text = text;
	textLabel.TextColor3 = Color3.fromRGB(255, 225, 255)
	textLabel.TextScaled = true;
	textLabel.TextXAlignment = Enum.TextXAlignment.Center;
	textLabel.TextYAlignment = Enum.TextYAlignment.Center;
	billboard.Parent = target
end;
local function clearEspLabels()
	for _, gui in ipairs(Workspace:GetDescendants()) do
		if gui:IsA("BillboardGui") and gui.Name == "ESP_Label" then
			gui:Destroy()
		end
	end
end;
local function updateEggEsp()
	local eggsFolder = Workspace:FindFirstChild("Interactions") and Workspace.Interactions:FindFirstChild("AbandonedEggs")
	if eggsFolder then
		for _, egg in ipairs(eggsFolder:GetChildren()) do
			if egg:IsA("Model") then
				createEspLabel(egg, "Abandoned Egg")
			end
		end
	end
end;
local function updateFoodEsp()
	local foodFolder = Workspace:FindFirstChild("Interactions") and Workspace.Interactions:FindFirstChild("Food")
	if foodFolder then
		for _, food in ipairs(foodFolder:GetChildren()) do
			if food:IsA("Model") then
				createEspLabel(food, food.Name)
			end
		end
	end
end;
local isEggEspEnabled = false;
local isFoodEspEnabled = false;
local isLavaRemoverEnabled = false;
local function refreshEsp()
	clearEspLabels()
	if isEggEspEnabled then
		updateEggEsp()
	end;
	if isFoodEspEnabled then
		updateFoodEsp()
	end
end;
local function lavaRemoverLoop()
	while isLavaRemoverEnabled do
		local lavaPools = Workspace:FindFirstChild("Interactions") and Workspace.Interactions:FindFirstChild("LavaPools")
		if lavaPools then
			for _, part in ipairs(lavaPools:GetChildren()) do
				if part:IsA("Part") then
					part:Destroy()
				end
			end
		end;
		task.wait(0.5)
	end
end;
local gameName = getGameName()
local window = uiLibrary:Create({
	Name = gameName .. " | MoonLight",
	StartupSound = {
		Toggle = false
	}
})
local infoTab = window:Tab("Info")
local mainTab = window:Tab("Main")
local uiTab = window:Tab("UI")
uiTab:Button("Destroy GUI", function()
	window:Exit()
end)
infoTab:Label("Script - Test")
infoTab:Button("Copy Telegram Link", function()
	setclipboard("https://t.me/henyscripts")
end)
mainTab:Button("Teleport to Eggs", function()
	local character = Players.LocalPlayer.Character;
	local rootPart = character and character:FindFirstChild("HumanoidRootPart")
	local eggModel = Workspace:FindFirstChild("Interactions") and Workspace.Interactions:FindFirstChild("AbandonedEggs") and Workspace.Interactions.AbandonedEggs:FindFirstChild("AbandonedEgg")
	if rootPart and eggModel then
		rootPart.CFrame = eggModel:GetPivot()
	else
		warn("Character or Egg Model not found!")
	end
end)
mainTab:Toggle("Egg ESP", function(enabled)
	isEggEspEnabled = enabled;
	refreshEsp()
end)
mainTab:Toggle("Food ESP", function(enabled)
	isFoodEspEnabled = enabled;
	refreshEsp()
end)
mainTab:Toggle("Lava Pool Remover", function(enabled)
	isLavaRemoverEnabled = enabled;
	if enabled then
		task.spawn(lavaRemoverLoop)
	end
end)
